import React, { useState } from 'react';

const Inputzin = ({ theme }) => {
    const [test, setTest] = useState(undefined)

    return (
        <div>
            {console.log(theme)}
            <input placeholder="Escreva Algo" onChange={(e) => { setTest(e.target.value) }} />
				<h1>
					{test}
				</h1>
        </div>
    );
}

export default Inputzin;
